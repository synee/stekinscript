func fib_0(x)
    func fib_add()
        return fib_0(x - 1) + fib_0(x - 2)
    return 1 if x < 2 else fib_add()

console.log(fib_0(0))
console.log(fib_0(1))
console.log(fib_0(2))
console.log(fib_0(3))
console.log(fib_0(4))

func fib_1(x)
    func fib_add()
        return fib_1(x - 1) + fib_1(x - 2)
    ifnot x < 2
        return fib_add()
    return 1

console.log(fib_1(5))
console.log(fib_1(6))
console.log(fib_1(7))
console.log(fib_1(8))
console.log(fib_1(9))
